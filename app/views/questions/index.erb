<% @questions.each do |question| %>
	<form action="/questions/<%= question.id %>/upvote" method="POST">
	  <button class="fa fa-thumbs-up inline" type="submit" name="question_vote" value="upvote_points"></button>
	</form>
	<form action="/questions/<%= question.id %>/downvote" method="POST">
	  <button class="fa fa-thumbs-down" type="submit" name="question_vote" value="downvote_points"></button>
	</form>

	Score: <%= question.vote_count %><br>

  <a href="/questions/<%=question.id%>"><%= question.text %></a>

  <form action="/questions/<%=question.id%>/edit" method="get">
    <button class="button" type="submit">Edit</button></br>
  </form>

  <form action="/questions/<%=question.id%>" method="post">
    <input type="hidden" name="_method" value="DELETE">
    <button class="button" type="submit">Delete</button>
  </form><br>

<% end %>
